#env:all


# OPENAI API的KEY，建议将该配置填写到系统环境变量中，以免泄露
OPENAI_API_KEY=


magicgpt.config.reader.threads.initialCount=100
magicgpt.config.reader.threads.maxThreads=2000
magicgpt.config.reader.threads.keepAliveTime.second=5
magicgpt.config.wizard.status.responding.message=我正在回答你的上一个问题，请稍等。
magicgpt.config.wizard.status.spelling.message=我正在处理问题，给我点时间哦。
magicgpt.config.network.vpn.enabled=false
# VPN的IP地址
magicgpt.config.network.vpn.host=
# VPN的服务端口
magicgpt.config.network.vpn.port=


# OpenAI Chat模型API地址
magicgpt.config.llm.api.openai.chat.url.default=https://api.openai.com/v1/chat/completions
# OPENAI Chat模型的镜像配置，如果你的项目是开源项目，建议将此项配置到环境变量中，以免泄露
magicgpt.config.llm.api.openai.chat.url.mirror=

magicgpt.config.llm.api.openai.chat.temperature=0.6

# openAI的steam接口读取超时设置(秒数)
magicgpt.config.llm.api.openai.timeout.read=10
# openAI的steam接口调用超时设置(秒数)
magicgpt.config.llm.api.openai.timeout.connect=10
# openAI的steam接口调用超时设置(秒数)
magicgpt.config.llm.api.openai.timeout.call=60

# openAI chat模型回答的最大长度。
magicgpt.config.llm.api.openai.chat.response.max.length=1000



# Search
# 如果设置为true，那么启动时如果搜索引擎不可用，启动将会被终止，为false时，则不会。
magicgpt.config.spell.search.engine.check = false
# 使用google custom search时分配的key，如果您的项目是开源项目，建议该值配置到环境变量中，避免信息泄露
magicgpt.config.spell.search.engine.google.api.key=
# 使用google custom search时分配的引擎id，如果您的项目是开源项目，建议该值配置到环境变量中，避免信息泄露
magicgpt.config.spell.search.engine.google.api.cx=
